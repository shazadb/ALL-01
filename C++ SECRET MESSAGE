#include <iostream>
#include <string>
using namespace std;

int main(){
    string pass = "ALLTASKC";
    string input;
    cout << "Enter Password for secret message: ";
    cin >> input;
    if (input==pass){
        cout << "Well done you have gained access to the secret message" << endl;
    }else{
        cout << "Wrong access denied cannot gain acess" << endl;
    }
    return 0;
}
